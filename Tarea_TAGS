    <img src="imagen.jpg" alt="Descripción de la imagen">

 <!--<div> - División:-->   

    <div>Contenido dividido</div>

     <!--<span> - Espacio en lineas:-->   
        <p>Este es un <span>texto en línea</span>.</p>
<!--<ul> - Lista desordenada:-->   
    <ul>
        <li>Elemento 1</li>
        <li>Elemento 2</li>
    </ul>
    
<!--<ol> - Lista ordenada:-->  
    <ol>
        <li>Elemento 1</li>
        <li>Elemento 2</li>
    </ol>

<!--<li> - Elemento de lista:-->  
    <ul>
        <li>Elemento 1</li>
        <li>Elemento 2</li>
        <li>Elemento 3</li>
    </ul>

<!--<table> - Tabla:-->  

    <table border="1">
        <tr>
            <td>Celda 1</td>
            <td>Celda 2</td>
        </tr>
        <tr>
            <td>Celda 3</td>
            <td>Celda 4</td>
        </tr>
    </table>

<!--<tr> - Fila de tabla:-->  

    <table border="1">
        <tr>
            <td>Celda 1</td>
            <td>Celda 2</td>
        </tr>
    </table>

<!--<td> - Celda de tabla:-->  

    <table border="1">
        <tr>
            <td>Celda 1</td>
            <td>Celda 2</td>
        </tr>
    </table>

<!--<form> - Formularios:-->  

    <form action="/submit-form" method="post">
        <label for="nombre">Nombre:</label>
        <input type="text" id="nombre" name="nombre"><br>
        <label for="email">Email:</label>
        <input type="email" id="email" name="email"><br>
        <input type="submit" value="Enviar">
    </form>

<!--<input> - Entrada:-->  
    <input type="text" name="nombre" id="nombre" value="John Doe">

<!--<butto> - Boton:-->  
    <button type="button">Haz clic aquí</button>

<!--<<textarea> - Area de Texto :-->  
    <textarea id="mensaje" name="mensaje" rows="4" cols="50">
        Escribe aquí tu mensaje...
        </textarea>

<!--<select> - Seleccion:-->  
<select name="colores" id="colores">
    <option value="rojo">Rojo</option>
    <option value="verde">Verde</option>
    <option value="azul">Azul</option>
</select>

<!--<option> - Opcion:-->
    <select name="colores" id="colores">
        <option value="rojo">Rojo</option>
        <option value="verde" selected>Verde</option>
        <option value="azul">Azul</option>
    </select>  

<!--<iframe> - Marca de incersion:-->  
<iframe src="https://www.ejemplo.com" width="600" height="400" frameborder="0"></iframe>


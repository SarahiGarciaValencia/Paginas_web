    <footer>
        <p>&copy; 2024 Ejemplo.com</p>
    </footer>

<!--<nav> - Navegación:-->  
    <nav>
        <ul>
            <li><a href="#">Inicio</a></li>
            <li><a href="#">Acerca de</a></li>
            <li><a href="#">Contacto</a></li>
        </ul>
    </nav>

<!--<section> - Seccion:-->  
<section>
    <h2>Sección Principal</h2>
    <p>Contenido de la sección principal.</p>
</section>

